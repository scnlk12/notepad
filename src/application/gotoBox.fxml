<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="149.0" prefWidth="306.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="application.gotoController">
   <children>
      <VBox layoutX="15.0" layoutY="12.0" prefHeight="87.0" prefWidth="275.0">
         <children>
            <Label text="行号(L):" />
            <TextField fx:id="textfield" />
         </children>
      </VBox>
      <GridPane layoutX="90.0" layoutY="87.0" prefHeight="54.0" prefWidth="206.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button fx:id="gotoButton" mnemonicParsing="false" onAction="#onGoToButtonClicked" prefHeight="36.0" prefWidth="88.0" text="转到" />
            <Button fx:id="quitButton" mnemonicParsing="false" onAction="#onQuitButtonClicked" prefHeight="36.0" prefWidth="88.0" text="取消" GridPane.columnIndex="1" />
         </children>
      </GridPane>
   </children>
</AnchorPane>
